<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Configuration -->
  <remote name="github" fetch="ssh://git@github.com/koromodako" review="" />
  <default revision="master" remote="github" sync-j="4" />
  <!-- Development Repositories -->
  <project path="datashark-dev" name="datashark-dev" groups="dev" />
  <!-- Production Repositories -->
  <project path="datashark-core" name="datashark-core" groups="prod,core" />
  <project path="datashark-service" name="datashark-service" groups="prod,service" />
  <project path="datashark-cli" name="datashark-cli" groups="prod,cli" />
  <!-- Production Plugin Repositories -->
  <project path="datashark-plugin-template" name="datashark-plugin-template" groups="prod,plugin,template" />
  <project path="datashark-plugin-tsk" name="datashark-plugin-tsk" groups="prod,plugin,extractor" />
  <project path="datashark-plugin-properties" name="datashark-plugin-properties" groups="prod,plugin,refiner" />
  <project path="datashark-plugin-vhd" name="datashark-plugin-vhd" groups="prod,plugin,extractor" />
  <project path="datashark-plugin-vmdk" name="datashark-plugin-vmdk" groups="prod,plugin,extractor" />
  <project path="datashark-plugin-windows" name="datashark-plugin-windows" groups="prod,plugin,refiner" />
  <project path="datashark-plugin-linux" name="datashark-plugin-linux" groups="prod,plugin,refiner" />
</manifest>
