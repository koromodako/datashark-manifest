<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Configuration -->
  <remote name="github" fetch="ssh://git@github.com/koromodako" review="" />
  <default revision="master" remote="github" sync-j="4" />
  <!-- Development Repositories -->
  <project path="datashark-dev" name="datashark-dev" groups="dev,independent" />
  <!-- Production Repositories -->
  <project path="datashark-core" name="datashark-core" groups="prod,core,independent" />
  <project path="datashark-agent" name="datashark-agent" groups="prod,agent,independent" />
  <project path="datashark-cli" name="datashark-cli" groups="prod,cli,independent" />
  <!-- Production Processors Repositories -->
  <project path="datashark-processors-independent" name="datashark-processors-independent" groups="prod,processors,independent" />
  <project path="datashark-processors-template" name="datashark-processors-template" groups="dev,processors,template" />
  <project path="datashark-processors-windows" name="datashark-processors-windows" groups="prod,processors,windows" />
  <project path="datashark-processors-linux" name="datashark-processors-linux" groups="prod,processors,linux" />
</manifest>
